  "bugs": {
    "url": "https://github.com/wesib/navigation/issues"
  },
  "type": "module",
  "main": "./dist/wesib.navigation.cjs",
  "module": "./dist/wesib.navigation.js",
  "types": "./index.d.ts",
  "exports": {
    "import": "./dist/wesib.navigation.js",
    "require": "./dist/wesib.navigation.cjs"
  },
  "peerDependencies": {
    "@frontmeans/namespace-aliaser": "^2.3.0",
    "@proc7ts/context-values": "^6.1.0",
    "@proc7ts/fun-events": "^10.4.0",
    "@wesib/generic": "^1.0.0-beta.43",
    "@wesib/wesib": "^1.0.0-beta.43"
  },
  "dependencies": {
    "@frontmeans/dom-events": "^1.1.0",
    "@frontmeans/dom-primitives": "^1.0.0",
    "@frontmeans/drek": "^1.1.0",
    "@frontmeans/render-scheduler": "^1.6.0",
    "@hatsy/http-header-value": "^3.5.3",
    "@proc7ts/amend": "^1.0.0-dev.5",
    "@proc7ts/primitives": "^2.2.0",
    "@proc7ts/push-iterator": "^2.5.0",
    "@proc7ts/supply": "^1.0.0"
  },
  "devDependencies": {
    "@frontmeans/namespace-aliaser": "^2.3.0",
    "@frontmeans/render-scheduler": "^1.5.0",
    "@proc7ts/context-values": "^6.1.0",
    "@proc7ts/fun-events": "^10.4.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@run-z/eslint-config": "^1.2.1",
    "@run-z/rollup-helpers": "^1.1.0",
    "@types/jest": "^26.0.23",
    "@types/jsdom": "^16.2.10",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "@wesib/generic": "^1.0.0-beta.43",
    "@wesib/wesib": "^1.0.0-beta.43",
    "cross-fetch": "^3.1.4",
    "eslint": "^7.27.0",
    "eslint-plugin-jest": "^24.3.6",
    "gh-pages": "^3.1.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "rollup": "^2.48.0",
    "rollup-plugin-flat-dts": "^1.2.1",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.30.0",
    "run-z": "^1.7.2",
    "shx": "^0.3.3",
    "ts-jest": "^26.5.6",
    "tslib": "^2.2.0",
    "typedoc": "^0.20.36",
    "typescript": "^4.3.1-rc"
  },
  "scripts": {
    "all": "run-z build,lint,test",
    "build": "run-z +z --then rollup --config ./rollup.config.js",
    "ci:all": "run-z all +test/--ci/--runInBand",
    "clean": "run-z +z --then shx rm -rf 'index.d.ts?(.map)' '*/index.d.ts?(.map)' dist target",
    "doc": "run-z +z --then typedoc",
    "doc:publish": "run-z doc --then gh-pages --dist target/typedoc --dotfiles",
    "lint": "run-z +z --then eslint . --ext .js,.ts",
    "test": "run-z +z --then jest",
    "z": "run-z +build,+doc,+lint,+test"
  }
}
